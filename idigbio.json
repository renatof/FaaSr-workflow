{
  "ActionList": {
    "start": {
      "Arguments": {
        "folder": "idigbio",
        "genus": "acer"
      },
      "InvokeNext": [],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "idigbio_media_download"
    }
  },
  "ComputeServers": {
    "GH": {
      "FaaSType": "GitHubActions",
