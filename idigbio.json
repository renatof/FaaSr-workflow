      "Writable": "TRUE",
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
  },
  "ActionContainers": {
    "start": "ghcr.io/faasr/github-actions-r:latest"
  },
  "FunctionInvoke": "start",
  "DefaultDataStore": "S3",
  "FunctionGitRepo": {
    "idigbio_media_download": "FaaSr/FaaSr-Functions"
  },
  "FaaSrLog": "FaaSrLog",
  "LoggingDataStore": "S3",
  "WorkflowName": "idigbio"
}