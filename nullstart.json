{
  "ActionList": {
    "start": {
      "Arguments": {
        "folder": "tutorial",
        "output1": "sample1.csv",
        "output2": "sample2.csv"
      },
      "InvokeNext": [
        "sum"
      ],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "create_sample_data"
    },
    "sum": {
      "FunctionName": "compute_sum",
      "FaaSServer": "GH",
      "Type": "R",
      "Arguments": {
        "folder": "tutorial",
        "input2": "sample2.csv",
        "input1": "sample1.csv",
        "output": "sum.csv"
      },
      "InvokeNext": []
    },
    "nullstart": {
      "InvokeNext": [
        "start"
      ],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "nullstart"
    }
  },
  "ComputeServers": {
    "GH": {
      "FaaSType": "GitHubActions",
      "UserName": "YOUR_USERNAME",
      "UseSecretStore": true,
      "ActionRepoName": "FaaSr-workflow",
      "Branch": "main"
    }
  },
  "DataStores": {
    "S3": {
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
  },
  "ActionContainers": {
    "start": "ghcr.io/faasr/github-actions-r:latest",
    "sum": "ghcr.io/faasr/github-actions-r:latest",
    "nullstart": "ghcr.io/faasr/github-actions-r:latest"
  },
  "FunctionInvoke": "nullstart",
  "DefaultDataStore": "S3",
  "FunctionGitRepo": {
    "create_sample_data": "FaaSr/FaaSr-Functions",
    "compute_sum": "FaaSr/FaaSr-Functions",
    "undefined": "FaaSr/FaaSr-Functions",
    "nullstart": "FaaSr/FaaSr-Functions"
  },
  "LoggingDataStore": "S3",
  "FaaSrLog": "FaaSrLog",
  "InvocationID": "tutorial",
  "WorkflowName": "nullstart"
}