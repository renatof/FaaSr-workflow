{
  "ActionList": {
    "start": {
      "Arguments": {
        "folder": "tutorialOW",
        "output1": "sample1.csv",
        "output2": "sample2.csv"
      },
      "InvokeNext": [
        "sum"
      ],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "create_sample_data"
    },
    "sum": {
      "FunctionName": "compute_sum",
      "FaaSServer": "OW",
      "Type": "R",
      "Arguments": {
        "folder": "tutorialOW",
        "input2": "sample2.csv",
        "input1": "sample1.csv",
        "output": "sum.csv"
      },
      "InvokeNext": []
    }
  },
    "ComputeServers": {
        "OW": {
            "FaaSType": "OpenWhisk",
            "Namespace": "guest",
            "UseSecretStore": false,
            "Endpoint": "ow.faasr.io:31002",
            "API.key": "filler:filler"
        },
        "GH": {
            "FaaSType": "GitHubActions",
            "UserName": "renatof",
            "UseSecretStore": true,
            "ActionRepoName": "FaaSr-workflow",
            "Branch": "main"
        }
    },
   "DataStores": {
    "S3": {
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
  },
  "ActionContainers": {
    "start": "ghcr.io/faasr/github-actions-r:latest",
    "sum": "faasr/openwhisk-r:latest"
  },
  "FunctionInvoke": "start",
  "DefaultDataStore": "S3",
  "FunctionGitRepo": {
    "create_sample_data": "FaaSr/FaaSr-tutorial",
    "compute_sum": "FaaSr/FaaSr-tutorial"
  },
  "LoggingDataStore": "S3",
  "FaaSrLog": "FaaSrLog",
  "InvocationID": "tutorialOW",
  "WorkflowName": "tutorialOW"
}
