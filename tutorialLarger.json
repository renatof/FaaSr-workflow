    "sumFinal": "ghcr.io/faasr/github-actions-r:latest"
  },
  "FunctionInvoke": "start",
  "DefaultDataStore": "S3",
  "FunctionGitRepo": {
    "create_sample_data": "FaaSr/FaaSr-Functions",
    "compute_sum": "FaaSr/FaaSr-Functions",
    "compute_mult": "FaaSr/FaaSr-Functions",
    "compute_div": "FaaSr/FaaSr-Functions",
